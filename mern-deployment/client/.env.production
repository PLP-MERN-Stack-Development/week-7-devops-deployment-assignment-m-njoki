# Client Environment Variables Template

# API Configuration
REACT_APP_API_URL=https://your-backend-domain.com/api
REACT_APP_ENVIRONMENT=production

# Authentication
REACT_APP_JWT_STORAGE_KEY=authToken
REACT_APP_REFRESH_TOKEN_KEY=refreshToken

# Features
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PWA=true

# Third-party Services
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key

# Social Authentication (Optional)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_GITHUB_CLIENT_ID=your-github-client-id

# Map Services (Optional)
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_MAPBOX_ACCESS_TOKEN=your-mapbox-token

# File Upload
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ACCEPTED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_VERSION=1.0.0

# Development vs Production
REACT_APP_DEBUG=false
REACT_APP_MOCK_API=false

# Branding
REACT_APP_BRAND_NAME=Your App Name
REACT_APP_BRAND_LOGO_URL=/logo.png
REACT_APP_BRAND_PRIMARY_COLOR=#007bff

# Contact Information
REACT_APP_CONTACT_EMAIL=support@your-domain.com
REACT_APP_SUPPORT_URL=https://your-domain.com/support

# Build Configuration
GENERATE_SOURCEMAP=false
BUILD_PATH=build
